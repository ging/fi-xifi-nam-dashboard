<script type="text/javascript">

$(function () {
	
	
    $('#results_graph').highcharts({
        chart: {
          marginBottom: 60
        },
        colors: ['#152859', '#666666', '#60C0CE', '#f7a35c', '#8085e9', '#f15c80', '#e4d354', '#8085e8', '#8d4653', '#91e8e1'],
        title: {
            text: 'One-Way Delay (ms)',
            align: 'left',
            margin: 30,
            style: { "color": "#152859", "fontSize": "13px" }
        },
        legend: {
          align: 'right',
          verticalAlign: 'bottom',
          borderWidth: 0,
          floating: true
        },
        xAxis: {
            categories: [
                <%=(isAvaliableSource.owd_data[1]).toString()%> , "2", "3", "4", "5"              
            ],
            plotBands: [{ // visualize the weekend
                from: -0.4,
                to: 6.4,
            }]
        },
        yAxis: {
          title: {
            text: null
          },
          labels: {
            style: {
              color: '#60C0CE'
            }
          }
        },
        tooltip: {
            shared: true,
            valueSuffix: ' ms'
        },
        credits: {
            enabled: false
        },
        plotOptions: {
            areaspline: {
                fillOpacity: 0.5
            }
        },
        series: [{
            name: 'Max',
            data: [<%=(isAvaliableSource.owd_data[1])%>,<%=(isAvaliableSource.owd_data[7])%>,<%=(isAvaliableSource.owd_data[13])%>,<%=(isAvaliableSource.owd_data[19])%>,<%=(isAvaliableSource.owd_data[25])%>]
        }, {
            name: 'Min',
            data: [<%=(isAvaliableSource.owd_data[0])%>,<%=(isAvaliableSource.owd_data[6])%>,<%=(isAvaliableSource.owd_data[12])%> ,<%=(isAvaliableSource.owd_data[18])%>,<%=(isAvaliableSource.owd_data[24])%>]
        },  {
            name: 'Jitter',
            data: [<%=(isAvaliableSource.owd_data[4])%>,<%=(isAvaliableSource.owd_data[10])%>,<%=(isAvaliableSource.owd_data[16])%>,<%=(isAvaliableSource.owd_data[22])%>,<%=(isAvaliableSource.owd_data[28])%>]
        }]
    });
});

</script>

<table class="table">  
  <thead>  
    <tr>  
      <th>ID</th>
      <th>Date</th>
      <th>Min</th>   
      <th>Max</th> 
      <th>Jitter</th>          
    </tr>  
  </thead>  
  <tbody>
    <%for (var d in isAvaliableSource.data) {%>
    <tr>  
      <th class="id_cell"><%=d%></th>  
      <td><%=((isAvaliableSource.data[d].date.replace("/", "")))%></td>
      <td><%=(isAvaliableSource.data[d].min ||)%> ms</td>  
      <td><%=(isAvaliableSource.data[d].max ||)%>  ms</td>  
      <td><%=(isAvaliableSource.data[d].jitter ||)%> ms</td>  
    </tr>
    <%}%>
  </tbody>  
</table>