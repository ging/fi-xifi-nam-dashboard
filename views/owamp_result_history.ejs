<!DOCTYPE html>

<script type="text/javascript">

$(function () {
	
	
    $('#results_graph').highcharts({
        chart: {
          marginBottom: 60
        },
        title: {
            text: 'One-Way Delay (ms)',
            align: 'left',
            margin: 30,
            style: { "color": "#152859", "fontSize": "13px" }
        },
        legend: {
          align: 'right',
          verticalAlign: 'bottom',
          borderWidth: 0,
          floating: true
        },
        xAxis: {
            categories: [
                <%=(isAvaliableSource.owd_data[1]).toString()%> , "2", "3", "4", "5"              
            ],
            plotBands: [{ // visualize the weekend
                from: -0.4,
                to: 6.4,
            }]
        },
        yAxis: {
          title: {
            text: null
          },
          labels: {
            style: {
              color: '#60C0CE'
            }
          }
        },
        tooltip: {
            shared: true,
            valueSuffix: ' ms'
        },
        credits: {
            enabled: false
        },
        plotOptions: {
            areaspline: {
                fillOpacity: 0.5
            }
        },
        series: [{
            name: 'Max',
            data: [<%=(isAvaliableSource.owd_data[1])%>,<%=(isAvaliableSource.owd_data[7])%>,<%=(isAvaliableSource.owd_data[13])%>,<%=(isAvaliableSource.owd_data[19])%>,<%=(isAvaliableSource.owd_data[25])%>]
        }, {
            name: 'Min',
            data: [<%=(isAvaliableSource.owd_data[0])%>,<%=(isAvaliableSource.owd_data[6])%>,<%=(isAvaliableSource.owd_data[12])%> ,<%=(isAvaliableSource.owd_data[18])%>,<%=(isAvaliableSource.owd_data[24])%>]
        },  {
            name: 'Jitter',
            data: [<%=(isAvaliableSource.owd_data[4])%>,<%=(isAvaliableSource.owd_data[10])%>,<%=(isAvaliableSource.owd_data[16])%>,<%=(isAvaliableSource.owd_data[22])%>,<%=(isAvaliableSource.owd_data[28])%>]
        }]
    });
});

    $('#result_modal').modal();
</script>

<table class="table">  
  <thead>  
    <tr>  
      <th>ID</th>
      <th>Date</th>
      <th>Min</th>   
      <th>Max</th> 
      <th>Jitter</th>          
    </tr>  
  </thead>  
  <tbody>  
    <tr>  
      <th class="id_cell">1</th>  
      <!--<td><%=(isAvaliableSource.date[0].match(/[-0-9]+/g)[0] || '')%></td> -->
      <td><%=((isAvaliableSource.date[0].replace("/", "")) || '')%></td>
      <td><%=(isAvaliableSource.owd_data[0] || '')%> ms</td>  
      <td><%=(isAvaliableSource.owd_data[1]||'')%>  ms</td>  
      <td><%=(isAvaliableSource.owd_data[4]|| '')%> ms</td>  
    </tr>  
    <tr>  
      <th class="id_cell">2</th>
      <td><%=((isAvaliableSource.date[1].replace("/", "")) || '')%></td> 
      <td><%=(isAvaliableSource.owd_data[6] || '')%> ms</td>  
      <td><%=(isAvaliableSource.owd_data[7]||'')%>  ms</td>  
      <td><%=(isAvaliableSource.owd_data[10]|| '')%> ms</td>  
    </tr>   
    <tr>  
      <th class="id_cell">3</th> 
      <td><%=((isAvaliableSource.date[2].replace("/", "")) || '')%></td> 
      <td><%=(isAvaliableSource.owd_data[12] || '')%> ms</td>  
      <td><%=(isAvaliableSource.owd_data[13]||'')%>  ms</td>  
      <td><%=(isAvaliableSource.owd_data[16]|| '')%> ms</td>  
    </tr>
    <tr>  
      <th class="id_cell">4</th>  
      <td><%=((isAvaliableSource.date[3].replace("/", "")) || '')%></td> 
      <td><%=(isAvaliableSource.owd_data[18] || '')%> ms</td>  
      <td><%=(isAvaliableSource.owd_data[19]||'')%>  ms</td>  
      <td><%=(isAvaliableSource.owd_data[22]|| '')%> ms</td>  
    </tr>
    <tr>  
      <th class="id_cell">5</th>  
      <td><%=((isAvaliableSource.date[4].replace("/", "")) || '')%></td>  
      <td><%=(isAvaliableSource.owd_data[24] || '')%> ms</td>  
      <td><%=(isAvaliableSource.owd_data[25]||'')%>  ms</td>  
      <td><%=(isAvaliableSource.owd_data[28]|| '')%> ms</td>  
    </tr>
  </tbody>  
</table>